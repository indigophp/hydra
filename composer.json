{
    "name": "indigophp/hydra",
    "description": "Easily convert array to object and object to array",
    "license": "MIT",
    "keywords": ["hydration", "extraction"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0",
        "athletic/athletic": "^0.1",
        "zendframework/zend-stdlib": "~2.3"
    },
    "suggest": {
        "zendframework/zend-stdlib": "Can be used with Zend hydrator"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Hydra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Hydra\\Stub\\": "stubs/",
            "Indigo\\Hydra\\Benchmark\\": "benchmarks/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
